{
  "extends": ["../.eslintrc.json", "plugin:react-hooks/recommended"],
  "env": {
    "browser": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react-refresh"],
  "rules": {
    "react-refresh/only-export-components": ["warn", { "allowConstantExport": true }]
  }
}
